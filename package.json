  "scripts": {
    "build": "yarn clean && yarn lint && yarn prettier:check && webpack --env.PRODUCTION",
    "build:analyzer": "yarn clean && webpack --env.PRODUCTION --env.ANALYZER",
    "clean": "rimraf dist && rimraf coverage",
    "lint": "tslint -c tslint.json 'src/**/*.ts'",
    "migration:run": "ts-node ./node_modules/typeorm/cli.js migration:run",
    "prettier:check": "prettier src/**/*.{html,js,scss,ts} *.{js,json,yml} --check",
    "prettier:write": "prettier src/**/*.{html,js,scss,ts} *.{js,json,yml} --write",
    "start": "yarn clean && set debug=* && ts-node-dev --respawn --transpileOnly ./src/index.ts",
    "u:i": "yarn upgrade-interactive --latest"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.2",
    "@types/body-parser": "^1.17.1",
    "@types/cors": "^2.8.6",
    "@types/helmet": "^0.0.45",
    "@types/jsonwebtoken": "^8.3.5",
    "@types/node": "^12.12.5",
    "prettier": "1.18.2",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.0",
    "sqlite3": "^4.1.0",
    "ts-loader": "6.2.1",
    "ts-node": "^8.4.1",
    "ts-node-dev": "^1.0.0-pre.43",
    "tslint": "^5.20.0",
    "typescript": "^3.6.4",
    "webpack": "^4.41.2",
    "webpack-bundle-analyzer": "^3.6.0",
    "webpack-cli": "^3.3.10",
    "webpack-dev-server": "^3.9.0"
  },
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.19.0",
    "class-validator": "^0.11.0",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "helmet": "^3.21.2",
    "jsonwebtoken": "^8.5.1",
    "typeorm": "^0.2.20"
  }
}
